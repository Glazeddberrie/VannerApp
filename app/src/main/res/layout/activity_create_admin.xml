<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/create_admin"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="413dp"
        android:layout_height="145dp"
        android:layout_marginBottom="20dp"
        android:contentDescription="si"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/circulo_admin" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="316dp"
        android:layout_height="52dp"
        android:layout_marginTop="164dp"
        android:layout_marginBottom="698dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Registrar Admin"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txt_nombreAdmin"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/txt_nombreAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_nombreAdmin"
        android:layout_width="293dp"
        android:layout_height="64dp"
        android:layout_marginTop="241dp"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txt_apellidoAdmin"
        app:layout_constraintStart_toStartOf="@+id/txt_apellidoAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_apellidoAdmin"
        android:layout_width="296dp"
        android:layout_height="64dp"
        android:layout_marginTop="323dp"
        android:ems="10"
        android:hint="Apellido"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txt_rutAdmin"
        app:layout_constraintStart_toStartOf="@+id/txt_rutAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_rutAdmin"
        android:layout_width="296dp"
        android:layout_height="64dp"
        android:layout_marginTop="401dp"
        android:ems="10"
        android:hint="Rut"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txt_emailAdmin"
        app:layout_constraintStart_toStartOf="@+id/txt_emailAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_emailAdmin"
        android:layout_width="293dp"
        android:layout_height="64dp"
        android:layout_marginTop="479dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txt_contraAdmin"
        app:layout_constraintStart_toStartOf="@+id/txt_contraAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_contraAdmin"
        android:layout_width="293dp"
        android:layout_height="64dp"
        android:layout_marginTop="558dp"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="@+id/txt_checkContraAdmin"
        app:layout_constraintStart_toStartOf="@+id/txt_checkContraAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txt_checkContraAdmin"
        android:layout_width="296dp"
        android:layout_height="64dp"
        android:layout_marginTop="639dp"
        android:layout_marginBottom="211dp"
        android:ems="10"
        android:hint="Confirmar Contraseña"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_registrarAdmin"
        app:layout_constraintStart_toStartOf="@+id/btn_registrarAdmin"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_registrarAdmin"
        android:layout_width="179dp"
        android:layout_height="64dp"
        android:layout_marginBottom="20dp"
        android:text="Registrar"
        app:layout_constraintBottom_toTopOf="@+id/btn_volverAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_volverAdmin"
        android:layout_width="179dp"
        android:layout_height="64dp"
        android:layout_marginTop="815dp"
        android:layout_marginBottom="35dp"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>